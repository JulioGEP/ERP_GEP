{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.8.0"
  },
  "scripts": {
    "generate": "PRISMA_SCHEMA_ENGINE_ADVISORY_LOCK_TIMEOUT=${PRISMA_SCHEMA_ENGINE_ADVISORY_LOCK_TIMEOUT:-60000} prisma generate --schema prisma/schema.prisma",
    "migrate:deploy": "node ../scripts/prisma-migrate-retry.js",
    "prisma:prune": "node ../scripts/prisma-prune-binaries.mjs || true",
    "typecheck": "tsc -p tsconfig.json",
    "postinstall": "prisma generate --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@netlify/functions": "^4.2.7",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "googleapis": "^133.0.0",
    "nodemailer": "^6.10.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.24",
    "prisma": "^5.22.0",
    "typescript": "^5.9.3"
  }
}
