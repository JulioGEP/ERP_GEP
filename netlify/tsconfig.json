{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": ["ES2020"],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmit": true,
  },
  "include": ["./functions/**/*.ts"],
  "exclude": ["../node_modules", "./**/*.js"]
}
